{
    "name": "tlm/readfile-filter",
    "description": "Exposes readfile() to twig template filter.",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "readfile"
    ],
    "support": {
        "docs": "https://github.com/EtienneTLM/craft-readfile-filter/blob/master/README.md",
        "issues": "https://github.com/EtienneTLM/craft-readfile-filter/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TLM",
            "homepage": "https://github.com/EtienneTLM"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "tlm\\readfilefilter\\": "src/"
        }
    },
    "extra": {
        "name": "Readfile Filter",
        "handle": "readfile-filter",
        "developer": "Etienne Bouchard",
        "developerUrl": "https://github.com/EtienneTLM",
        "documentationUrl": "https://github.com/EtienneTLM/craft-readfile-filter/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/EtienneTLM/craft-readfile-filter/master/CHANGELOG.md",
        "class": "tlm\\readfilefilter\\ReadfileFilter"
    }
}